{
	"name": "oops/google-authenticator",
	"description": "Two-factor authenticator via Google Authenticator app.",
	"keywords": ["security", "authentication", "2fa", "totp"],
	"type": "library",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Jiří Pudil",
			"email": "me@jiripudil.cz",
			"homepage": "http://jiripudil.cz"
		}
	],
	"support": {
		"email": "me@jiripudil.cz",
		"issues": "https://github.com/o2ps/GoogleAuthenticator/issues"
	},
	"require": {
		"php": ">= 5.4.0",
		"nette/utils": "~2.2"
	},
	"require-dev": {
		"nette/tester": "~1.0"
	},
	"suggest": {
		"nette/di": "Optional integration with Nette's DI container"
	},
	"autoload": {
		"psr-0": {
			"Oops\\GoogleAuthenticator\\": "src/"
		},
		"classmap": [
			"src/Oops/GoogleAuthenticator/exceptions.php"
		],
		"files": [
			"src/Oops/GoogleAuthenticator/DI/compatibility.php"
		]
	}
}
